<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundTextView">
        <attr name="rtvCornerRadius" format="dimension" />
        <attr name="rtvBgColor" format="color" />
        <attr name="rtvTextColor" format="color" />
        <attr name="rtvHorizontalPadding" format="dimension" />
        <attr name="rtvVerticalPadding" format="dimension" />
        <attr name="rtvStrokeWidth" format="dimension" />
        <attr name="rtvStrokeColor" format="color" />
    </declare-styleable>


    <declare-styleable name="RoundConstraintLayout">
        <attr name="rcl_cornerRadius" format="dimension" />
        <attr name="rcl_backgroundColor" format="color" />
        <attr name="rcl_strokeColor" format="color" />
        <attr name="rcl_strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="StatusLightView">
        <attr name="slv_connectedColor" format="color" />
        <attr name="slv_disconnectedColor" format="color" />
        <attr name="slv_connectingColor" format="color" />
    </declare-styleable>


</resources>